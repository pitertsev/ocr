<div class="wrapper-btns">
  <div class="btns">
    <div class="select-pictures">
      <form id="form-file" enctype="multipart/form-data" id="send-image" action="/" method="post">
        <div class="file-field input-field">
          <div class="waves-effect file-btn">
            <span>Выберите файл</span>
            <input type="file" id="file" name="file">
          </div>
          <div class="file-path-wrapper">
            <input class="file-path validate" type="text" placeholder="Файл не выбран">
          </div>
        </div>
        <div class="img"></div>
        <button class="waves-effect my-btn hide" type="submit" id="start">Узнать что там написано</button>
      </form>
    </div>
    <select class="browser-default">
    <option value="" disabled selected>Выберите голос</option>
    <option value="1">Option 1</option>
    <option value="2">Option 2</option>
    <option value="3">Option 3</option>
  </select>
    <div class="btn-bottom hide">
      <button class="waves-effect my-btn" id="read-btn">Читай</button>
      <button class="waves-effect my-btn" id="cancel-read">Стоп</button>
    </div>
  </div>
</div>
<div class="progress hide">
  <div class="indeterminate"></div>
</div>
<div id="text-container" class="hide">
</div>
